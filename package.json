    "@types/uuid": "^9.0.0",
    "ts-node-dev": "^2.0.0",
    "tsnd": "^1.1.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "argon2": "^0.30.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.1.4",
    "typeorm": "^0.3.11",
    "uuid": "^9.0.0"
  },
  "scripts": {
    "dev": "ts-node-dev src/server.ts",
    "typeorm": "typeorm-ts-node-common-js",
    "migration:create": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:create",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run -d ./ormconfig.ts",
    "migration:revert": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:revert"
  }
